{
	"apps": [{
		"name": "shift-explorer-socket-server",
		"script": "app.js",
		"cwd": ".",
		"pid_file": "./pids/explorer-socket-server.app.pid",
		"out_file": "./logs/explorer-socket-server.app.log",
		"error_file": "./logs/shift.app.err",
